<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://code.alibabatech.com/schema/dubbo
       http://code.alibabatech.com/schema/dubbo/dubbo.xsd">


    <dubbo:application name="dubbo-consumer"/>

    <!--不使用注册中心-->
    <dubbo:reference id="helloService" interface="com.learning.service.HelloService" url="dubbo://localhost:20880" />


    <!--使用zookeeper注册中心-->
    <dubbo:registry address="zookeeper://192.168.116.132:2181" />

    <dubbo:reference id="helloService1" interface="com.learning.service.HelloService" />

</beans>